{
  "name": "nuxt-typescript-template",
  "description": "A boilerplate to start a Nuxt+TS project with Composition API quickly",
  "private": true,
  "author": "Yusuf Ada <yusufadaa@gmail.com> (https://github.com/ysfada)",
  "repository": "github:ysfada/nuxt-typescript-template",
  "scripts": {
    "dev:nuxt": "nuxt",
    "dev:nest": "nest start --watch",
    "prebuild": "rimraf .nest .nuxt",
    "build": "nest build && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "format": "prettier --write --ignore-path .gitignore .",
    "lint": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:fix": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore --fix ."
  },
  "dependencies": {
    "@nestjs/common": "7.6.17",
    "@nestjs/core": "7.6.17",
    "@nestjs/platform-express": "7.6.17",
    "@nuxtjs/composition-api": "0.24.0",
    "@vue/runtime-dom": "3.0.11",
    "express": "4.17.1",
    "nuxt": "2.15.6",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "socket.io": "4.1.2",
    "socket.io-client": "4.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nuxt/types": "2.15.6",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/style-resources": "1.0.0",
    "@types/express": "4.17.12",
    "@types/node": "14.17.1",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.9.0",
    "prettier": "2.3.0",
    "sass": "1.34.0",
    "sass-loader": "10.1.1"
  }
}
